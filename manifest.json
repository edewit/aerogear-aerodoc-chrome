{
    "name": "AeroDoc Chrome",
    "description": "AeroDoc web build for Chrome",
    "version": "0.1",
    "manifest_version": 2,
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "permissions": [
